.regist-header
  .regist-header__inner
    .wrapper
      %h1.wrapper__logo
        = link_to root_path, class: "wrapper__logo__furima" do
          = image_tag "material/logo/logo.png", class: "stylist-image1"
      %nav.user-header__nav
        %ol.user-header__nav__ol
          %li.through.user-header__nav__ol__progress
            会員情報
            .user-header__nav__ol__progress__li
          %li.through.user-header__nav__ol__progress
            住所登録
            .user-header__nav__ol__progress__li
          %li.active.user-header__nav__ol__progress
            完了
            .user-header__nav__ol__progress__li

.infomation-input
  .MemberInformationInput
    .MemberInformationInput__title
      会員登録完了
    .MemberInformationInput__content
      .MemberInformationInput__content--form
        %p.completed__text
          ありがとうございます。
        %p.completed__text
          会員登録が完了しました。

        %button{type: "submit", class: "btn_red_chomp"}
          = link_to "アプリをはじめる", root_path


.item__sell__footer
  %ul
    %li
      プライバシーポリシー
    %li
      フリマ利用規約
    %li
      特定商取引に関する表記
  .item__sell__footer__logo
    = link_to root_path do
      = image_tag "/assets/material/logo/logo-white.png", class: "furima__white__logo"